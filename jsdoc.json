{
    "source": {
        "include": [
            "./docs/README.md",
            "./client/js/controller",
            "./client/js/module",
            "./client/js/service"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "staticFiles": {
                "include": [
                    "./docs"
                ]
            }
        }
    },
    "opts": {
        "recurse": true,
        "destination": "./jsdocs"
    },
    "plugins": [
        "plugins/markdown"
    ]
}
